global store = [];

function clearStore() {
	for (var key : var value in store) {
		if (contains(key, 'tmp-')) {
			removeKey(store, key);
		}
	}
}

function getInStore(key) {
	return store[key];
}

function hasInStore(key) {
	if (store[key] !== null) {
		return true;
	}
	// if in store but with null value
	for (var k: var v in store) {
		if (k === key) {
			return true;
		}
	}
	return false;
}

function putInStore(key, value) {
	store[key] = value;
}
